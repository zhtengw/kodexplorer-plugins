{
	"id":"drawio",
	"name":"{{LNG.drawio.meta.name}}",
	"title":"{{LNG.drawio.meta.title}}",
	"version":"2.0",
	"category":"file",
	"source":{
		"icon":"{{pluginHost}}static/images/icon.png",
		"screenshoot":[
			"{{pluginHost}}static/images/screenshot1.png",
			"{{pluginHost}}static/images/screenshot2.png",
			"{{pluginHost}}static/images/screenshot3.png"
		]
	},
	"description":"{{LNG.drawio.meta.desc}}",
	"auther":{
		"copyright":"zhtengw",
		"homePage":"https://github.com/zhtengw/kodexplorer-plugins"
	},
	"configItem":{
		"formStyle":{
			"className":"",
			"tabs":[
				{
					"name":"{{LNG.Plugin.tab.basic}}",
					"field":["pluginAuth","sep001","openWith","fileExt","fileSort"]
				}
			]
		},
		"pluginAuth":{
			"type":"userSelect",
			"value":"all:1",
			"display":"{{LNG.Plugin.config.auth}}",
			"desc":"{{LNG.Plugin.config.authDesc}}",
			"require":1
		},
		"sep001":"<hr/>",
		"openWith":{
			"type":"radio",
			"value":"dialog",
			"display":"{{LNG.Plugin.Config.openWith}}",
			"info":[
				["dialog","{{LNG.Plugin.Config.openWithDilog}}"],
				["window","{{LNG.Plugin.Config.openWithWindow}}"]
			]
		},
		"fileExt":{
			"type":"tags",
			"display":"{{LNG.Plugin.Config.fileExt}}",
			"desc":"{{LNG.Plugin.Config.fileExtDesc}}",
			"value":"xml,drawio"
		},
		"fileSort":{
			"type":"number",
			"display":"{{LNG.Plugin.Config.fileSort}}",
			"desc":"{{LNG.Plugin.Config.fileSortDesc}}",
			"value":100
		},
		"theme":{
			"type":"select",
			"value":"kennedy",
			"display":"{{LNG.drawio.Config.Theme}}",
			"info":[
				["kennedy","Kennedy"],
				["minimal","Minimal"],
				["atlas","Atlas"],
				["dark","Dark"]
			]
        },
		"autoSave":{
			"type":"switch",
			"value":1,
			"display":"{{LNG.auto_save}}"
        },
        "serverAddr":{
			"type":"input",
			"value":"https://www.draw.io",
			"display":"{{LNG.drawio.Config.serverAddr}}",
			"desc":"{{LNG.drawio.Config.serverAddrDesc}}"
        }
	}
}
